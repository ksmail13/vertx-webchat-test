<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="MyApp">
    <Appenders>
        <Console name="Console">
            <PatternLayout>
                <Pattern>%d [%p] %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>

        </Console>
    </Appenders>

    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
